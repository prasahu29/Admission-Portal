<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

<br><br>
<section class="login-wrap mtb-40">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-12">
                <center class="login-box">
                    <center>
                        <h2 style="font-family: Times New Roman Times, serif ;">ALL Registrations </h2>
                    </center>
                    <br>

                    <a class="btn btn-outline-primary" href="/logout">Log out</a>
                    <br><br><br>
                    <table class="table table-striped table-hover">
                        <thead>

                            <tr>

                                <th>Name</th>
                                <th>Email</th>
                                <th>Number</th>
                                <th>College</th>
                                <th>Address</th>
                                <th>Course</th>
                                <!-- <th>status</th> -->
                                <th>Actions</th>

                            </tr>
                        </thead>

                        <tbody>
                            <% data1.forEach((item)=> { %>
                                <tr>

                                    <td>
                                        <h4 style="font-family: Times New Roman Times, serif ;">
                                            <%= item.name %>
                                        </h4>
                                    </td>
                                    <td>

                                        <h4 style="font-family: Times New Roman Times, serif ;">
                                            <%= item.email %>
                                        </h4>
                                    </td>
                                    <td>
                                        <h4 style="font-family: Times New Roman Times, serif ;">
                                            <%= item.number%>
                                        </h4>
                                    </td>
                                    <td>
                                        <h4 style="font-family: Times New Roman Times, serif ;">
                                            <%= item.college%>
                                        </h4>
                                    </td>
                                    <td>
                                        <h4 style="font-family: Times New Roman Times, serif ;">
                                            <%=item.address%>
                                        </h4>
                                    </td>
                                    <td>
                                        <h4 style="font-family: Times New Roman Times, serif ;">
                                            <%= item.course%>
                                        </h4>
                                    </td>

                                    <td>
                                        <a class="btn btn-outline-primary"
                                            href="/admin_college/edit/<%= item._id %>">Edit </a>
                                        <br>
                                        <a class="btn btn-outline-primary"
                                            href="/admin_college/delete/<%= item._id %>">Delete </a>
                                    </td>

                                    <td>



                                    </td>


                                </tr>
                                <%})%>
                        </tbody>

                    </table>

                    <a class="btn btn-outline-primary" href="/">Back To Log-In Page</a><br>


                    <br>

            </div>
        </div>
    </div>
    </div>
</section>